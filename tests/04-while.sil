(#extern puts (#types/func [#types/string #types/int]))

(#define int_sub (#func [(#types/int self) (#types/int other) (#types/int)] {
    (#untyped "return self - other;")
}))

(#define main (#func [() (#types/int)] (#do {
    (#define x 10)
    (#while
        (#do {
            (#define y x)
            (#set x (int_sub x 1))
            y
        })
        (#do {
            (puts "TEST\n")
        })
    )
    0
})))
