(#expect puts (#types/func [#types/string #types/int]))
(#actual puts "c" "extern int puts(char const *)")

(#define Int.dec (#func [(#types/int self) (#types/int)] {
    (#untyped "c" "self - 1")
}))

(#define main (#func [() (#types/int)] (#do {
    (#define x 10)
    (#while
        (#do {
            (#define y x)
            (#set x (Int.dec x))
            y
        })
        (#do {
            (puts "TEST")
        })
    )
    0
})))
