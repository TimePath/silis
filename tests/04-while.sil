(#define Int.dec (#func [(#types/int self) (#types/int)] {
    (#untyped "self - 1")
}))

(#extern puts (#types/func [#types/string #types/int]))

(#define main (#func [() (#types/int)] (#do {
    (#define x 10)
    (#while
        (#do {
            (#define y x)
            (#set x (Int.dec x))
            y
        })
        (#do {
            (puts "TEST\n")
        })
    )
    0
})))
